{
  "kafkaTopic": "%s",
  "format": "AVRO",
  "from": 1,
  "to": %s,
  "chunkSize": 1000,
  "schema": {
    "name": "transaction",
    "type": "record",
    "fields": [
      {
        "name": "transaction_id",
        "type": "int"
      },
      {
        "name": "transaction_date",
        "type": {
          "type": "string",
          "avro.java.string": "String"
        },
        "default": "2020-08-12"
      },
      {
        "name": "account_id",
        "type": "int"
      },
      {
        "name": "amount",
        "type": "int"
      }
    ]
  }
}